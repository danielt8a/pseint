Algoritmo formulaGeneral
	
	Definir a, b, c, valorPositivo, valorNegativo, root Como Real;
	
	Escribir "Ingrese el valor de a";
	Leer a;
	
	Escribir "Ingrese el valor de b";
	Leer b;
	
	Escribir "Ingrese el valor de c";
	Leer c;
	
	valorPositivo <- (-b + rc((b^2) - (4 * a * c) )) / (2 * a) ;
	
	valorNegativo <- (-b - rc((b^2) - (4 * a * c))) / (2 * a);
	
	Escribir "x1 es igual a ",valorPositivo;
	
	Escribir "x2 es igual a ",valorNegativo;
	
FinAlgoritmo
